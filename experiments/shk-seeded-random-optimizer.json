{
    "title": "TADAM Random",
    "worker_config": {
        "memory": 8,
        "cpu": 2,
        "gpu": 1,
        "command": ["python3", "-m", "model.tadam", "--log_dir=/logs", "--number_of_steps=30000"],
        "volumes": [
            "/mnt/datasets/public/:/data",
            "/mnt/home/thomas/logs/tadam:/logs"
        ]
    },
    "parameters": {
        "name": "RandomSeed",
        "config": {
            "strategy": {
                "name": "RandomSearch",
                "config": {
                    "dimensions": {
                        "train_batch_size": "randint(16, 64)",
                        "pre_train_batch_size": "randint(32, 64)",
                        "init_learning_rate": "norm(0.1, 0.02)",
                        "lr_decay_rate": "norm(10, 1)",
                        "optimizer": ["sgd", "adam"]
                    },
                    "nb_trials": 15
                }
            },
            "random_seeds": [7654, 5678, 8765, 6789, 9876, 12345, 54321, 23456, 65432, 34567, 76543, 45678, 87654, 56789, 98765],
            "repeat": 10
        }
    }
}
